{
  "name": "sitemap-urls",
  "version": "2.0.0",
  "description": "Extract URLs from an XML sitemap.",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [
    "sitemap",
    "url",
    "parse",
    "extract"
  ],
  "repository": "Rowno/sitemap-urls",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": {
    "sitemap-urls": "lib/cli.js"
  },
  "scripts": {
    "test": "jest --ci && xo",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "cheerio": "0.22.0",
    "get-stdin": "^5.0.0",
    "meow": "^3.0.0",
    "update-notifier": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "execa": "^0.7.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.2",
    "xo": "^0.18.2"
  },
  "files": [
    "lib"
  ],
  "xo": {
    "space": true,
    "semicolon": false,
    "overrides": [
      {
        "files": "**/__tests__/**/*.js",
        "envs": [
          "jest"
        ]
      }
    ]
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ]
  }
}
